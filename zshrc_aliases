alias mvim="mvim -v"
alias composer="php /usr/local/bin/composer.phar"
alias findconflicts="find . -name \"*copy: conflict*\" -print"
alias rmconflicts="find . -name \"*copy: conflict*\" -print -delete"
alias rmdsf="find . -name \".DS_Store\" -print -delete"
alias chext="phpcs . --standard=MEQP2"
alias exttr="phpcs . --standard=MEQP2 --severity=10 --extensions=php,phtml"
alias packext="~/Workspace/dotfiles/scripts/packext.sh"
